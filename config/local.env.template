# Database Configuration
DATABASE_URL=postgresql+asyncpg://username:password@localhost:5432/database_name
DB_SCHEMA=your_schema_name
DB_POOL_SIZE=10
DB_MAX_OVERFLOW=20

# JWT Configuration
JWT_SECRET_KEY=your_jwt_secret_key_here
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=30

# Application Configuration
LOG_LEVEL=DEBUG
ENVIRONMENT=local
API_PREFIX=/interview/api/v1
CORS_ORIGINS=["http://localhost:3000", "http://localhost:8080"]

# Pipecat API Keys (add your actual keys here)
# Note: google_api_key is used for both Google LLM and Gemini services
google_api_key=your_google_api_key_here
deepgram_api_key=your_deepgram_api_key_here

# AWS SQS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
SQS_INTERVIEW_COMPLETION_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/your-account-id/InterviewCompletion

# Logging control
DISABLE_WEBRTC_DEBUG=true
